<h1>Explore Latest</h1>

<div class="row row-cols-2 row-cols-lg-5 g2 g-lg-3">
  <% if (recipe != '') { %> <% recipe.forEach(function(recipe, index) { %>
  <a href="/recipe/<%= recipe.id %>" class="col text-center category__link">
    <div class="category__img category__image--large shadow">
      <img src="/uploads/<%= recipe.image %>" alt="<%- recipe.name %>" loading="lazy" />
    </div>
    <div class="pt-1"><%- recipe.name %></div>
  </a>
  <% }) %> <% } %>
</div>
